l1 = [1,2,3,4,5]
l2 = [6,5,4,3,2]
l3=[]
for i in range(0,5):
  if l1[i]>l2[i]:l3.append(l1[i])
  else:
    l3.append(l2[i])
print(l3)



l1 = [1,2,3,4,5]
l2 = [6,5,4,3,2]
l3 = [False,False,True,False,True]
l4=[]
for i in range(0,5):
  if l3[i]==True:l4.append(l1[i])
  else:
    l4.append(l2[i])
print(l4)
